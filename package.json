{
  "name": "techie-service",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "clean": "rm -rf ./dist"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.3",
    "@nestjs/bull": "^10.2.1",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.2.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.2.0",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.2.0",
    "@nestjs/platform-express": "^10.2.0",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/throttler": "^4.2.1",
    "@nestjs/typeorm": "^10.0.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "exceljs": "^4.4.0",
    "helmet": "^7.0.0",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "moment": "^2.30.1",
    "nanoid": "^5.0.9",
    "nestjs-i18n": "^10.3.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.5.5",
    "@nestjs/cli": "^10.1.12",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.0",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20.5.1",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.3",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.filter.(t|j)s",
      "!**/*.response.(t|j)s",
      "!**/*.guard.(t|j)s",
      "!**/*.interceptor.(t|j)s",
      "!**/*.controller.(t|j)s",
      "!**/*.module.(t|j)s",
      "!**/*.option.(t|j)s",
      "!**/*.config.(t|j)s",
      "!**/*.dto.(t|j)s",
      "!src/main.(t|j)s",
      "!src/public/**/*.*",
      "!src/view/*.hbs",
      "!src/application/decorator/*.(t|j)s",
      "!src/application/listener/*.(t|j)s",
      "!src/infrastructure/io/dto/*.(t|j)s",
      "!src/infrastructure/io/entity/*.(t|j)s",
      "!src/migration/*.(t|j)s",
      "!src/public/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@utility(|/.*)$": "<rootDir>/src/utility/$1",
      "^@application(|/.*)$": "<rootDir>/src/application/$1",
      "^@request(|/.*)$": "<rootDir>/src/application/request/$1",
      "^@response(|/.*)$": "<rootDir>/src/application/response/$1",
      "^@config(|/.*)$": "<rootDir>/src/_config/$1",
      "^@constant(|/.*)$": "<rootDir>/src/constant/$1",
      "^@domain(|/.*)$": "<rootDir>/src/domain/$1",
      "^@service(|/.*)$": "<rootDir>/src/domain/service/$1",
      "^@io(|/.*)$": "<rootDir>/src/infrastructure/io/$1",
      "^@entities(|/.*)$": "<rootDir>/src/infrastructure/io/entity/$1",
      "^@infrastructure(|/.*)$": "<rootDir>/src/infrastructure/$1"
    }
  }
}
